{
  "name": "syncing",
  "displayName": "Syncing",
  "description": "Sync every settings with your own GitHub Gist.",
  "version": "1.4.7",
  "publisher": "nonoroazoro",
  "author": {
    "email": "xiaochao.k@gmail.com",
    "name": "nonoroazoro",
    "url": "http://www.alibabagroup.com/"
  },
  "homepage": "https://github.com/nonoroazoro/vscode-syncing",
  "license": "MIT",
  "main": "src/extension",
  "icon": "src/res/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nonoroazoro/vscode-syncing"
  },
  "bugs": {
    "url": "https://github.com/nonoroazoro/vscode-syncing/issues",
    "email": "xiaochao.k@gmail.com"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "sync",
    "syncing",
    "vscode-syncing"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "syncing.uploadSettings",
        "title": "Syncing: Upload Settings"
      },
      {
        "command": "syncing.downloadSettings",
        "title": "Syncing: Download Settings"
      },
      {
        "command": "syncing.openSettings",
        "title": "Syncing: Open Syncing Settings"
      }
    ],
    "keybindings": [
      {
        "key": "",
        "command": "syncing.uploadSettings"
      },
      {
        "key": "",
        "command": "syncing.downloadSettings"
      },
      {
        "key": "",
        "command": "syncing.openSettings"
      }
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.6.1",
    "eslint-config-zoro": "^1.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "mocha": "^3.5.3",
    "should": "^13.0.1",
    "vscode": "^1.1.5"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "async": "^2.5.0",
    "fs-extra": "^4.0.2",
    "github": "^8.1.1",
    "https-proxy-agent": "^2.1.0",
    "moment": "^2.18.1",
    "temp": "^0.8.3"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "git add"
    ]
  }
}
