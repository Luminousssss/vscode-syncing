{
  "name": "syncing",
  "displayName": "syncing",
  "description": "Sync VSCode Settings (including Extensions) to GitHub Gist.",
  "version": "1.1.4",
  "publisher": "nonoroazoro",
  "license": "MIT",
  "main": "src/extension",
  "icon": "src/res/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nonoroazoro/syncing"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Syncing Configuration",
      "properties": {
        "syncing.proxy": {
          "type": "string",
          "default": "",
          "description": "Proxy setting which is used by Syncing."
        }
      }
    },
    "commands": [
      {
        "command": "syncing.uploadSettings",
        "title": "Syncing: Upload Settings"
      },
      {
        "command": "syncing.downloadSettings",
        "title": "Syncing: Download Settings"
      },
      {
        "command": "syncing.openSettings",
        "title": "Syncing: Open Syncing Settings"
      }
    ],
    "keybindings": [
      {
        "key": "",
        "command": "syncing.uploadSettings"
      },
      {
        "key": "",
        "command": "syncing.downloadSettings"
      },
      {
        "key": "",
        "command": "syncing.openSettings"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.7.1",
    "eslint-config-zoro": "^1.1.4",
    "eslint-plugin-react": "^6.4.1",
    "mocha": "^3.1.2",
    "should": "^11.1.1",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "async": "^2.1.2",
    "fs-extra": "^0.30.0",
    "github": "^5.2.1",
    "https-proxy-agent": "^1.0.0",
    "temp": "^0.8.3"
  }
}